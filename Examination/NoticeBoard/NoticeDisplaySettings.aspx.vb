    Private Sub Button1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button1.Click
        Try
            getFormValues()
            addValuesToTable()

        Catch ex As Exception
            Throw ex
        End Try


    End Sub

    Private Function getFormValues()
        Try

            sRowbackcolor = rowBackColor.SelectedItem.Text
            sRowForecolor = rowForeColor.SelectedItem.Text
            iRowfontsize = DrpFontSizeRow.SelectedItem.Value
            sRowfamily = rowFontName.SelectedItem.Text

            If rbHand.Checked = True Then
                rBullet = "hand.gif"
            ElseIf rbRight.Checked = True Then
                rBullet = "noticeArrow.gif"
            ElseIf rbMesg.Checked = True Then
                rBullet = "msg.gif"
            End If


            If rbNew.Checked = True Then
                rAlert = "new_alert.gif"
            ElseIf rbAlert.Checked = True Then
                rAlert = "exclam.gif"
            End If
        Catch ex As Exception

            Throw ex

        End Try


    End Function

    Private Function addValuesToTable()
        Try

            Table1.Rows(2).Cells(0).BackColor = Color.FromName(sRowbackcolor)
            Table1.Rows(2).Cells(0).ForeColor = Color.FromName(sRowForecolor)
            Table1.Rows(2).Cells(0).Font.Size = FontUnit.Point(iRowfontsize)
            Table1.Rows(2).Cells(0).Font.Name = sRowfamily

        Catch ex As Exception
            Throw ex
        End Try



    End Function

    Private Sub Button2_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button2.Click

        Try
            getFormValues()
            Dim rowDispSettings As String
            rowDispSettings = sRowbackcolor & "," & sRowForecolor & "," & sRowfamily & "," & iRowfontsize & "," & rBullet & "," & rAlert
            objMessage = New ClsMessageBoard
            objMessage.MB_Message_Fonts(CInt(MSGSLNO), Session("USERSLNO"), rowDispSettings)


        Catch ex As Exception
            Throw ex
        End Try

        '' Button2.Attributes.Add("OnClick", "dispClose()")


    End Sub

