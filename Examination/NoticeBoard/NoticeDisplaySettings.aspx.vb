    Private Sub Page_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        Try

            If Not IsPostBack Then
                FormInfo(Session("USERSLNO"), Session("USERLOGSLNO"), System.Reflection.MethodBase.GetCurrentMethod().ReflectedType.FullName)
                MSGSLNO = Request.QueryString("MSGSLNO")
                Session("MSGSLNO") = MSGSLNO
            Else
                MSGSLNO = Session("MSGSLNO")

            End If
            AddFonts()

        Catch ex As Exception
            Throw ex
        End Try
        

    End Sub


    Private Sub AddFonts()
        Try

            Dim Fonts As New System.Drawing.Text.InstalledFontCollection
            Dim Family As FontFamily
            For Each Family In Fonts.Families
                ''THFontName.Items.Add(Family.Name)
                rowFontName.Items.Add(Family.Name)
            Next
            Dim i As Integer
            For i = 8 To 35
                ''THFontSize.Items.Add(i)
                DrpFontSizeRow.Items.Add(i)

            Next

            AddrowBackColor()
            AddrowForeColor()

        Catch ex As Exception
            Throw ex
        End Try
        ' Get the installed fonts collection.
       

    End Sub

